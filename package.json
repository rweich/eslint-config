{
  "name": "@rweich/eslint-config",
  "version": "1.1.4",
  "description": "eslint config for my projects",
  "author": "rweich",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'test/**/*'",
    "prepare": "husky install",
    "test": "tape -r ts-node/register/transpile-only test/**/*.Test.ts"
  },
  "peerDependencies": {
    "typescript": "^4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "prettier": "npm:@btmills/prettier"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@rweich/semantic-release-config": "^2.0.0",
    "@types/eslint": "^8.2.1",
    "@types/tape": "^4.13.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "semantic-release": "^18.0.0",
    "tape": "^5.2.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=14.17"
  },
  "repository": "rweich/eslint-config",
  "files": [
    "README.md",
    "index.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "coding",
    "rules",
    "style"
  ],
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "extends": "@rweich/semantic-release-config",
    "branches": "main"
  },
  "lint-staged": {
    "index.js": [
      "eslint --quiet --fix"
    ]
  }
}
